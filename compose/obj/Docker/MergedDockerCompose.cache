networks:
  dockersql-network:
    driver: bridge
services:
  dockersql.application.webapi:
    build:
      context: C:\Projects\workshop-docker\compose
      dockerfile: Example.WebApi/Dockerfile
    depends_on:
    - mysql.database
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: dockersql.application.webapi
    networks:
      dockersql-network: {}
    ports:
    - published: 9091
      target: 80
  mysql.database:
    environment:
      MYSQL_ROOT_PASSWORD: 123Aa321
    image: mysql
    networks:
      dockersql-network: {}
    ports:
    - published: 3306
      target: 3306
version: '3.6'